cko from '@/helpers/coingecko.json';\r\n\r\nexport default {\r\n  props: ['open'],\r\n  data() {\r\n    return {\r\n      input: '',\r\n      coingecko\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState(['settings'])\r\n  },\r\n  methods: {\r\n    select(value, i) {\r\n      value.i = i;\r\n      this.$emit('input', value);\r\n      this.$emit('close');\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]}]}